{
    "categories": [
        {
            "title": "## üöÄ Features",
            "labels": [
                "feature",
                "feat",
                "chore(enhancement)",
                "feat(enhancement)",
                "chore(documentation)",
                "feat(i8n)"
            ]
        },
        {
            "title": "## üêõ Fixes",
            "labels": [
                "fix",
                "feat(fix)"
            ]
        },
        {
            "title": "## üì¶ Dependencies",
            "labels": [
                "chore(deps)",
                "fix(deps)",
                "feat(github-action)",
                "fix(js)",
                "fix(container)",
                "feat(container)",
                "feat(js)"
            ]
        },
        {
            "title": "## üß™ Tests",
            "labels": [
                "test"
            ]
        },
        {
            "title": "## üöÄ CI/CD Workflows",
            "labels": [
                "ci"
            ]
        },
        {
            "title": "## üöÄ Build / Deploy",
            "labels": [
                "chore(build)"
            ]
        },
        {
            "title": "## Other",
            "exhaustive": true,
            "exhaustive_rules": "false",
            "empty_content": "- no matching PRs",
            "rules": [
                {
                    "pattern": "open",
                    "on_property": "status",
                    "flags": "gu"
                }
            ]
        }
    ],
    "label_extractor": [
        {
            "method": "regexr",
            "pattern": "(.+?)!?:.+",
            "target": "$1",
            "on_property": "title"
        }
    ],
    "sort": {
        "order": "ASC",
        "on_property": "mergedAt"
    },
    "template": "#{{CHANGELOG}}\n\n<details>\n<summary>Uncategorized</summary>\n\n#{{UNCATEGORIZED}}\n</details>",
    "pr_template": "- #{{TITLE}} by @#{{AUTHOR}} in #{{URL}}",
    "empty_template": "- no changes",
    "transformers": [
        {
            "pattern": "[\\-\\*] (\\[(...|TEST|CI|SKIP)\\])( )?(.+?)\n(.+?[\\-\\*] )(.+)",
            "target": "- $4\n  - $6"
        }
    ],
    "trim_values": false,
    "max_tags_to_fetch": 200,
    "max_pull_requests": 200,
    "max_back_track_time_days": 365,
    "tag_resolver": {
        "method": "semver"
    }
}